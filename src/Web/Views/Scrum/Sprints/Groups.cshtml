@{
    ViewBag.Title = "Sprints";
    var groupPairs = ViewData["GroupPairs"] as List<KeyValuePair<string, string>>;
}

<div class="row">
    <div class="page-header">
        <h1>Report By Project</h1>
    </div>
</div>

<div class="row">
    <h2>Groups Overview</h2>
    @foreach (var item in groupPairs)
    {
        <div class="col-md-3">
            <a href="~/Scrum/Index?groupId=@Html.UrlEncoder.Encode(item.Key)">@item.Value</a>
        </div>
    }
</div>